{
  "override": false,
  "lifelines": [
    {
      "name": ".*Controller.*",
      "entryPoint": true
    },
    {
      "name": ".*Service.*"
    },
    {
      "name": ".*Repository.*",
      "annotation": ".*(Repository|Named)",
      "dbAccess": true
    },
    {
      "name": ".*Factory.*",
      "lifelineOnly": true
    },
    {
      "name": ".*Specification.*",
      "lifelineOnly": true
    }
  ],
  "sqlLogPattern": {
    "start": ".*org.hibernate.SQL.*",
    "end": "^[0-9]{4}-.*"
  }
}